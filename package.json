{
  "name": "n8n-nodes-digicryptostore",
  "version": "1.0.0",
  "description": "DigiCryptoStore - Secure document certification and archiving on Solana blockchain with permanent Arweave storage. Developed by CHECKHC.",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "digicryptostore",
    "checkhc",
    "blockchain",
    "solana",
    "arweave",
    "irys",
    "nft",
    "certification",
    "document",
    "digital-vault",
    "permanent-storage"
  ],
  "license": "MIT",
  "homepage": "https://www.checkhc.net",
  "author": "CHECKHC <contact@checkhc.net> (https://checkhc.net)",
  "repository": {
    "type": "git",
    "url": "https://github.com/checkhc/n8n-nodes-digicryptostore.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier credentials nodes --write",
    "lint": "eslint credentials nodes package.json",
    "lintfix": "eslint credentials nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
